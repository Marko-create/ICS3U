progname = "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Duis in."

# Initialize a counter for spaces
space_count = 0

# Loop through each character in the string
for c in progname:
    if c == " ":  # Check if the character is a space
        space_count += 1  # Increment the counter

# Print the result
print(f"There are {space_count} spaces in the text.")
