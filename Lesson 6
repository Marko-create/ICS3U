year_of_birth = int(input("Enter your year of birth: "))

age = int(input("Enter your age: "))

step1_result = (year_of_birth * 2) + 5

step2_result = (step1_result * 50) + age

final_result = (step2_result - 250) / 100

print(f"The final result is: {final_result}")
print(f"This result represents your age ({age}) followed by the last two digits of your birth year ({year_of_birth % 100})")
