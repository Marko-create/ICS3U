import random

# Step 1: Create the initial array
size = 6
B = [i for i in range(1, size + 1)]

# Step 2: Define the shuffle function
def shuffle(A):
    for i in range(len(A)):
        # Choose a random index to swap with
        j = random.randint(0, len(A) - 1)
        # Swap the elements
        A[i], A[j] = A[j], A[i]
    return A

# Shuffle the array and print the result
shuffled_array = shuffle(B)
print(shuffled_array)
